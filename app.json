{
  "expo": {
    "name": "careConnect",
    "slug": "careConnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/doctor.png",
    "scheme": "careconnect",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/doctor.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rahul12121.careConnect"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/doctor.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "67a3abca-889b-4baa-af9e-2465ca8cfc6a"
      }
    },
    "owner": "rahul12121"
  }
}
